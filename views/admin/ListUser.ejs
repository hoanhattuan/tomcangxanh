
<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head.ejs %>
</head>

<body>
<div id="wrapper">
    <% include partials/nav.ejs %>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Day la trang admin</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="table-responsive">          
                        <table class="table">
                          <thead>
                            <tr>
                              <th>ID</th>
                              <th>Họ tên</th>
                              <th>Tài khoản</th>
                              <th>Số điện thoại</th>
                              <th>Email</th>
                              <th>Ngày sinh</th>
                              <th><a href="/nguoiquantri/nguoidung/them"><span>Thêm</span></a></th>
                            </tr>
                          </thead>
                          <tbody>
                          <% users.forEach(function(rs){%>
                            <tr>
                              <td><%= rs.user_id %></td>
                              <td><%= rs.user_fullName %></td>
                              <td><%= rs.user_userName %></td>
                              <td><%= rs.user_phone %></td>
                              <td><%= rs.user_email %></td>
                              <td><%= rs.user_birthday %></td>
                              <td><a href="/nguoiquantri/nguoidung/sua/<%= rs.user_id %>" ><span>Sửa</span></a></td>
                            </tr>
                            <% }); %>
                          </tbody>
                        </table>
                        </div>
                </div>
                <!-- /.col-lg-12 -->
            </div>
        </div>
        <!-- /#page-wrapper -->

    </div>
	
	
    <% include partials/scripts.ejs %>
</body>
</html>
